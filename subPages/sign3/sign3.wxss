.stage-3-0{
  animation-name: stage3;
  animation-duration: 1.6s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-1{
  animation-name: stage3;
  animation-duration: 1.9s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-2{
  animation-name: stage3;
  animation-duration: 2.2s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-3{
  animation-name: stage3;
  animation-duration: 2.5s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
@keyframes stage3{
  0%{
    transform: translate(0);
  }
  100%{
    transform: translate(-1500px,0);
    display: none;
  }
}
.stage-3-first-1{
  animation-name: stage33;
  animation-duration: 1.4s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-first-2{
  animation-name: stage33;
  animation-duration: 1.7s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-first-3{
  animation-name: stage33;
  animation-duration: 2s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
.stage-3-first-4{
  animation-name: stage33;
  animation-duration: 2.1s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
  animation-iteration-count: 1;
}
@keyframes stage33{
  0%{
    transform: translate(-1000rpx,0);
  }
  100%{
    transform: translate(0,0);
    
  }
}
.form>.stage3{
  position: absolute;
  left: 0;
  top: 0;
  width: 70vw;
  height: 40vh;
  border-radius: 40rpx;
  background: #2980b980;
  border:1px solid #00000080;
  margin-bottom: 0;
  animation-duration: 1.8s;
  animation-name: stage333;
  animation-fill-mode: forwards;
}
@keyframes stage333{
  0%{
    opacity: 0;
    transform: translate(800rpx,0);
  }
  100%{
    opacity: 1;
    transform: translate(0,0);
  }
}
.form>.stage3>view{
  border-radius: 20rpx;
  width: 80%;
  height: 80%;
  background: #ffffff;
  padding: 10rpx;
}
.hide{
  /* display: none; */
  /* opacity: 0; */
  animation-name: hide;
  animation-duration: .3s;
  animation-fill-mode: forwards;
}
@keyframes hide{
  0%{
    opacity: .1;
  }
  100%{
    z-index: -1;
    opacity: 0;
  }
}
.two-1{
  z-index: 4;
  animation-name: goLeft;
  animation-duration: .7s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
}
.one-2{
  z-index: 4;
  animation-name: goRight;
  animation-duration: .7s;
  animation-fill-mode: forwards;
  animation-timing-function: ease-in-out;
}
@keyframes goLeft{
  0%{
    opacity: .5;
    transform: translate(500rpx,0);
  }
  100%{
    opacity: 1;
    transform:  translate(0,0);
  }
}
@keyframes goRight{
  0%{
    opacity: .5;
    transform: translate(-500rpx,0);
  }
  100%{
    opacity: 1;
    transform:  translate(0,0);
  }
}
.iconSuccess{
  animation-name: rotatel;
  animation-fill-mode: forwards;
  animation-duration: .6s;
  animation-timing-function: ease-in-out;
  color:#20ee5e !important;
}
.iconFail{
  animation-name: rotater;
  animation-fill-mode: forwards;
  animation-duration: .6s;
  animation-timing-function: ease-in-out;
  color:#f51e1e !important;
}
@keyframes rotater{
  0%{
    transform: rotate(0);
  }
  100%{
    transform: rotate(360deg);
  }
}
@keyframes rotatel{
  0%{
    transform: rotate(0);
  }
  100%{
    transform: rotate(-360deg);
  }
}
.stage4{
  z-index: 3;
  background: #00000080;
}
.stage3-4{
  z-index: 10;
}
.first{
  z-index: 4;
}